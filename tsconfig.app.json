{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "types": ["react"],
    "module": "ESNext",
   "moduleResolution": "Bundler",
    "target": "ESNext",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
